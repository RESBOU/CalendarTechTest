{
  "name": "rescal",
  "version": "1.0.0",
  "description": "resbou calendar",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/browserify -t babelify clientside/index.jsx -o static/js/index.js",
    "pack": "./node_modules/.bin/uglifyjs static/js/index.js -o static/js/index.min.js -c 'dead_code=true,evaluate=true,join_vars=true,unused=true,drop_console=true' -m 'toplevel,sort'",
    "make": "npm run clean; npm run build && npm run pack",
    "clean": "rm static/js/*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "js-data": "^2.8.2",
    "js-data-http": "^2.1.2",
    "js-data-mongodb": "^0.5.1",
    "js-data-redis": "^2.3.0",
    "mongodb": "^2.1.6",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-flow-strip-types": "^6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "uglify-js": "^2.6.1"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  }
}
